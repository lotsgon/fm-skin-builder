<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
  <ui:BindingRoot>
    <ui:VisualElement class="sicard units-block-unit-details">
      <ui:SIText class="global-text-primary heading-large-22px divider-horizontal-margin-8" text-binding="[VisualFunction]"/>
      <ui:SIScrollView class="base-template-grow scroller-default">
        <ui:VisualElement class="margin-right-global-gap-regular">
          <ui:VisualElement class="divider-horizontal-margin-8 frame-generic-with-padding">
            <ui:VisualElement class="row-direction-normal divider-horizontal-margin-8">
              <ui:SIText class="global-text-primary heading-regular-18px" text-binding="[VisualFunction]"/>
            </ui:VisualElement>
            <ui:VisualElement>
              <ui:SIRadioGroup class="two-columns-buttons-mode center center-radio-button-template-content ring-radio-button-group unity-group-box database-custom-setup-radio-group-disabled" selection-binding="setupgame.SelectedDatabaseSize"/>
            </ui:VisualElement>
          </ui:VisualElement>
          <ui:VisualElement class="divider-horizontal-margin-8 frame-generic-with-padding">
            <ui:VisualElement class="row-direction-normal divider-horizontal-margin-8">
              <ui:SIText class="global-text-primary heading-regular-18px" text-binding="[VisualFunction]"/>
              <ui:VisualElement>
                <ui:SIDropdown class="buttons-dropdown-icon-base-default">
                  <ui:SIText class="button-regular content-ellipsis content-no-wrap" text-binding="[VisualFunction]"/>
                  <ui:VisualElement class="ui-concepts-chevron-down_small margin-right-global-gap-none margin-left-global-gap-small"/>
                </ui:SIDropdown>
              </ui:VisualElement>
            </ui:VisualElement>
            <ui:VisualElement class="divider-horizontal-margin-8">
              <ui:BindableSwitchElement data-binding="[VisualFunction]">
                <ui:SIText class="global-text-secondary body-regular-14px-regular" text-binding="[VisualFunction]"/>
                <ui:GridSelector class="comp-site-child-only-grid-selector margin-right-global-gap-none margin-left-global-gap-none padding-left-global-padding-none"/>
              </ui:BindableSwitchElement>
            </ui:VisualElement>
          </ui:VisualElement>
          <ui:VisualElement class="divider-horizontal-margin-8 frame-generic-with-padding">
            <ui:VisualElement class="row-direction-normal divider-horizontal-margin-8">
              <ui:SIText class="global-text-primary heading-regular-18px" text-binding="[VisualFunction]"/>
              <ui:VisualElement>
                <ui:SIDropdown class="buttons-dropdown-icon-base-default">
                  <ui:SIText class="button-regular content-ellipsis content-no-wrap" text-binding="[VisualFunction]"/>
                  <ui:VisualElement class="ui-concepts-chevron-down_small margin-right-global-gap-none margin-left-global-gap-small"/>
                </ui:SIDropdown>
              </ui:VisualElement>
            </ui:VisualElement>
            <ui:VisualElement class="divider-horizontal-margin-8">
              <ui:BindableSwitchElement data-binding="[VisualFunction]">
                <ui:SIText class="global-text-secondary body-regular-14px-regular" text-binding="[VisualFunction]"/>
                <ui:GridSelector class="comp-site-child-only-grid-selector margin-right-global-gap-none margin-left-global-gap-none padding-left-global-padding-none"/>
              </ui:BindableSwitchElement>
            </ui:VisualElement>
          </ui:VisualElement>
          <ui:VisualElement class="divider-horizontal-margin-8 frame-generic-with-padding">
            <ui:VisualElement class="row-direction-normal divider-horizontal-margin-8">
              <ui:SIText class="global-text-primary heading-regular-18px" text-binding="[VisualFunction]"/>
              <ui:VisualElement>
                <ui:SIDropdown class="buttons-dropdown-icon-base-default">
                  <ui:SIText class="button-regular content-ellipsis content-no-wrap" text-binding="[VisualFunction]"/>
                  <ui:VisualElement class="ui-concepts-chevron-down_small margin-right-global-gap-none margin-left-global-gap-small"/>
                </ui:SIDropdown>
              </ui:VisualElement>
            </ui:VisualElement>
            <ui:VisualElement class="divider-horizontal-margin-8">
              <ui:BindableSwitchElement data-binding="[VisualFunction]">
                <ui:SIText class="global-text-secondary body-regular-14px-regular" text-binding="[VisualFunction]"/>
                <ui:GridSelector class="comp-site-child-only-grid-selector margin-right-global-gap-none margin-left-global-gap-none padding-left-global-padding-none"/>
              </ui:BindableSwitchElement>
            </ui:VisualElement>
          </ui:VisualElement>
          <ui:VisualElement class="divider-horizontal-margin-8 frame-generic-with-padding">
            <ui:VisualElement class="row-direction-normal divider-horizontal-margin-8">
              <ui:SIText class="global-text-primary heading-regular-18px" text-binding="[VisualFunction]"/>
              <ui:VisualElement>
                <ui:SIDropdown class="buttons-dropdown-icon-base-default">
                  <ui:SIText class="button-regular content-ellipsis content-no-wrap" text-binding="[VisualFunction]"/>
                  <ui:VisualElement class="ui-concepts-chevron-down_small margin-right-global-gap-none margin-left-global-gap-small"/>
                </ui:SIDropdown>
              </ui:VisualElement>
            </ui:VisualElement>
            <ui:VisualElement class="divider-horizontal-margin-8">
              <ui:BindableSwitchElement data-binding="[VisualFunction]">
                <ui:SIText class="global-text-secondary body-regular-14px-regular" text-binding="[VisualFunction]"/>
                <ui:GridSelector class="comp-site-child-only-grid-selector margin-right-global-gap-none margin-left-global-gap-none padding-left-global-padding-none"/>
              </ui:BindableSwitchElement>
            </ui:VisualElement>
          </ui:VisualElement>
        </ui:VisualElement>
      </ui:SIScrollView>
      <ui:VisualElement>
        <ui:TemplateContainer class="divider-horizontal-margin-8" template="layout-divider-solid-horizontal"/>
        <ui:VisualElement class="divider-horizontal-margin-8">
          <ui:VisualElement class="row-direction-normal center">
            <ui:SIText class="global-text-primary body-regular-14px-bold margin-right-global-gap-small" text-binding="[VisualFunction]"/>
            <ui:SIText class="global-text-secondary body-regular-14px-regular" text-binding="[VisualFunction]"/>
          </ui:VisualElement>
        </ui:VisualElement>
        <ui:TemplateContainer class="center divider-horizontal-margin-8" template="notifications-modal-dialog-actions-default"/>
      </ui:VisualElement>
    </ui:VisualElement>
  </ui:BindingRoot>
</ui:UXML>