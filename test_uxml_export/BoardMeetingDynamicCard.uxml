<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
  <ui:BindingRoot>
    <ui:SIStyleSetter>
      <ui:BindingVariables class="flex-grow-class">
        <ui:BindingRemapper binding-mappings="human=game.GameCurrentManager;conversation=Person.ConversationEditSession;speakto=conversation.SpeakTo;humanclub=human.PrimaryClubTeam.Club">
          <ui:BindingRemapper binding-mappings="club=humanclub">
            <ui:TemplateContainer template="FullScreenExperienceTitleBar"/>
          </ui:BindingRemapper>
          <ui:TemplateContainer class="flex-grow-class" template="FigmaBaseMeetingTemplate"/>
        </ui:BindingRemapper>
      </ui:BindingVariables>
    </ui:SIStyleSetter>
  </ui:BindingRoot>
</ui:UXML>