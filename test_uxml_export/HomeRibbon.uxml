<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
  <ui:BindingRoot class="home-ribbon-root">
    <ui:SIGradient class="ios-quick-action-background"/>
    <ui:BindingVariables>
      <ui:BindingRemapper binding-mappings="id=ribbon-id">
        <ui:SISafeAreaElement>
          <ui:SIButton class="shortcut-fake-button flex-grow-class row-direction-normal">
            <ui:Ribbon/>
            <ui:TemplateContainer class="margin-right-global-gap-regular" template="CurrentDayWidget"/>
            <ui:TutorialAnchorElement class="continue-button-container">
              <ui:TemplateContainer class="continue-button-container" template="dashboard-hud-continue_button-default"/>
            </ui:TutorialAnchorElement>
          </ui:SIButton>
        </ui:SISafeAreaElement>
      </ui:BindingRemapper>
    </ui:BindingVariables>
  </ui:BindingRoot>
</ui:UXML>