<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
  <ui:BindingRoot>
    <ui:BindingRemapper binding-mappings="human=game.GameCurrentManager;boolean=const.True">
      <ui:VisualElement>
        <ui:SITile class="sicard">
          <ui:SIImage/>
          <ui:VisualElement>
            <ui:VisualElement>
              <ui:VisualElement class="row-direction-normal margin-right-global-gap-large margin-left-global-gap-large">
                <ui:SIAspectRatioFitter>
                  <ui:SIButton class="sibutton margin-right-global-gap-none margin-left-global-gap-none padding-right-global-padding-none padding-left-global-padding-none"/>
                </ui:SIAspectRatioFitter>
                <ui:SIText class="sitile__title white" text-binding="[VisualFunction]"/>
              </ui:VisualElement>
            </ui:VisualElement>
            <ui:VisualElement>
              <ui:TemplateContainer template="layout-divider-solid-horizontal"/>
              <ui:SIButton/>
              <ui:TemplateContainer template="layout-divider-solid-horizontal"/>
              <ui:VisualElement>
                <ui:SIText class="margin-right-global-gap-extra-large margin-left-global-gap-extra-large" text-binding="[VisualFunction]"/>
                <ui:VisualElement>
                  <ui:BindingVariables>
                    <ui:StreamedObjectList data-binding="human.HumanTopStories"/>
                  </ui:BindingVariables>
                </ui:VisualElement>
              </ui:VisualElement>
              <ui:TemplateContainer class="margin-right-global-gap-regular margin-left-global-gap-regular" template="layout-divider-solid-horizontal"/>
              <ui:VisualElement>
                <ui:SIText class="margin-right-global-gap-regular margin-left-global-gap-regular"/>
                <ui:VisualElement class="row-direction-normal">
                  <ui:SIVisible data-binding="[VisualFunction]">
                    <ui:VisualElement class="ui-concepts-globe_small margin-right-global-gap-regular"/>
                  </ui:SIVisible>
                  <ui:BindableSwitchElement data-binding="data.PortalNewsTitleTextStyle">
                    <ui:SIText class="heading-xsmall-14px  heading-left-align heading-no-wrap margin-right-global-gap-small" text-binding="[VisualFunction]"/>
                    <ui:SIText class="heading-xsmall-14px  heading-left-align heading-no-wrap margin-right-global-gap-small" text-binding="[VisualFunction]"/>
                    <ui:VisualElement/>
                  </ui:BindableSwitchElement>
                  <ui:SIVisible data-binding="[VisualFunction]">
                    <ui:VisualElement class="ui-concepts-chevron-right_small"/>
                  </ui:SIVisible>
                </ui:VisualElement>
                <ui:VisualElement class="row-direction-normal">
                  <ui:VisualElement>
                    <ui:SIButton/>
                    <ui:SIButton/>
                  </ui:VisualElement>
                  <ui:VisualElement>
                    <ui:SIButton/>
                    <ui:SIButton/>
                    <ui:SIButton/>
                  </ui:VisualElement>
                  <ui:VisualElement>
                    <ui:SIButton/>
                    <ui:SIButton/>
                  </ui:VisualElement>
                </ui:VisualElement>
              </ui:VisualElement>
            </ui:VisualElement>
          </ui:VisualElement>
        </ui:SITile>
      </ui:VisualElement>
    </ui:BindingRemapper>
  </ui:BindingRoot>
</ui:UXML>