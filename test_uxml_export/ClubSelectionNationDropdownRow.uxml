<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
  <ui:BindingExpect class="row-direction-normal">
    <ui:SIText class="body-regular-14px-regular margin-right-global-gap-regular" text-binding="row.DisplayValue"/>
    <ui:BindingRemapper class="center" binding-mappings="dbnation=row.PropertyValue">
      <ui:SIVisible data-binding="dbnation.AvailableGenders">
        <ui:SIText class="body-regular-14px-regular global-text-secondary" text-binding="[VisualFunction]"/>
      </ui:SIVisible>
    </ui:BindingRemapper>
  </ui:BindingExpect>
</ui:UXML>