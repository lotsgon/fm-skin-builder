<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
  <ui:BindingRoot>
    <ui:BindingRemapper binding-mappings="human=game.GameCurrentManager;pressconference=Human.PressConferenceEditSession;speakto=pressconference.SpeakTo">
      <ui:VisualElement>
        <ui:SIAspectRatioFitter class="row-direction-normal">
          <ui:VisualElement>
            <ui:VisualElement class="row-direction-normal margin-right-global-gap-large margin-left-global-gap-large">
              <ui:SIAspectRatioFitter>
                <ui:SIButton class="sibutton margin-right-global-gap-none margin-left-global-gap-none padding-right-global-padding-none padding-left-global-padding-none"/>
              </ui:SIAspectRatioFitter>
              <ui:SIText class="title"/>
            </ui:VisualElement>
            <ui:SIText class="title"/>
            <ui:SIText text-binding="pressconference.ConversationRoomAtmosphere.InteractionBodyLanguageString"/>
            <ui:SIText class="sisubtitle"/>
            <ui:SIText text-binding="pressconference.NumberOfQuestionsAsked"/>
            <ui:SIText text-binding="pressconference.NumberOfQuestionsRemaining"/>
            <ui:SIText text-binding="pressconference.TotalNumberOfQuestions"/>
            <ui:StreamedObjectList data-binding="pressconference.JournalistsAttending"/>
            <ui:SIText text-binding="pressconference.NumberOfJournalistsAttending"/>
          </ui:VisualElement>
          <ui:TemplateContainer template="SpeakToSidePanel"/>
        </ui:SIAspectRatioFitter>
      </ui:VisualElement>
    </ui:BindingRemapper>
  </ui:BindingRoot>
</ui:UXML>