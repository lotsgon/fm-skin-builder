<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
  <ui:BindingExpect class="base-template-grow">
    <ui:VisualElement>
      <ui:BindableSwitchElement data-binding="[VisualFunction]">
        <ui:SIText class="heading-regular-18px" text-binding="[VisualFunction]"/>
        <ui:SIText class="heading-regular-18px" text-binding="[VisualFunction]"/>
      </ui:BindableSwitchElement>
    </ui:VisualElement>
    <ui:VisualElement>
      <ui:VisualElement>
        <ui:VisualElement class="row-direction-normal bottom-margin-8">
          <ui:VisualElement class="game-concepts-club-badge_small margin-right-global-gap-regular"/>
          <ui:SIText class="heading-small-16px global-text-secondary" text-binding="SetupGame.SelectedContinent.MajorClubsString"/>
        </ui:VisualElement>
        <ui:VisualElement class="row-direction-normal">
          <ui:VisualElement class="row-direction-normal base-template-grow">
            <ui:SIText class="global-text-secondary body-small-12px-bold margin-right-global-gap-regular margin-left-global-gap-regular" text-binding="[VisualFunction]"/>
            <ui:SIText class="global-text-secondary body-small-12px-bold" text-binding="[VisualFunction]"/>
          </ui:VisualElement>
          <ui:SIText class="global-text-secondary body-small-12px-bold margin-right-global-gap-huge" text-binding="[VisualFunction]"/>
        </ui:VisualElement>
        <ui:TemplateContainer template="layout-divider-solid-horizontal"/>
        <ui:StreamedObjectList data-binding="SetupGame.SelectedContinent.MajorClubs"/>
      </ui:VisualElement>
    </ui:VisualElement>
    <ui:VisualElement class="divider-horizontal-margin-16">
      <ui:VisualElement class="row-direction-normal bottom-margin-8">
        <ui:VisualElement class="margin-right-global-gap-regular"/>
        <ui:SIText class="heading-small-16px global-text-secondary" text-binding="[VisualFunction]"/>
      </ui:VisualElement>
      <ui:BindingVariables>
        <ui:StreamedObjectList data-binding="SetupGame.SelectedContinent.TopPlayersInRegion"/>
      </ui:BindingVariables>
    </ui:VisualElement>
    <ui:VisualElement>
      <ui:VisualElement class="row-direction-normal bottom-margin-8">
        <ui:VisualElement class="margin-right-global-gap-regular"/>
        <ui:SIText class="heading-small-16px global-text-secondary" text-binding="SetupGame.SelectedContinent.MajorCompetitionsString"/>
      </ui:VisualElement>
      <ui:StreamedObjectList data-binding="SetupGame.SelectedContinent.Competitions"/>
    </ui:VisualElement>
  </ui:BindingExpect>
</ui:UXML>