<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
  <ui:BindingRoot class="base-template-grow">
    <ui:BindingRemapper binding-mappings="filebrowser=data.SaveGameFileBrowserReference">
      <ui:BindingVariables>
        <ui:SIVisible class="base-template-grow" data-binding="config.ShowPhonePanels">
          <ui:TemplateContainer class="base-template-grow" template="FigmaBaseCardTemplate">
            <ui:TemplateContainer class="margin-right-global-gap-extra-large margin-left-global-gap-extra-large" template="SaveGameBody"/>
          </ui:TemplateContainer>
        </ui:SIVisible>
        <ui:SIVisible class="base-template-grow" data-binding="config.ShowPhonePanels">
          <ui:TemplateContainer class="base-template-grow" template="FigmaBaseCardTemplate"/>
          <ui:TemplateContainer class="quick-action-buttons-template-layout" template="QuickActionMenuButtons"/>
        </ui:SIVisible>
      </ui:BindingVariables>
    </ui:BindingRemapper>
  </ui:BindingRoot>
</ui:UXML>