<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
  <ui:BindingRoot class="base-template-grow">
    <ui:BindingVariables class="center">
      <ui:VisualElement class="notifications--notifications-modal-large-default-default">
        <ui:VisualElement>
          <ui:VisualElement>
            <ui:VisualElement class="margin-bottom-16 ios-margin-bottom-0">
              <ui:BindableSwitchElement data-binding="config.HideNationStrings">
                <ui:SIText class="heading-large-22px global-text-primary" text-binding="[VisualFunction]"/>
                <ui:SIText class="heading-large-22px global-text-primary" text-binding="[VisualFunction]"/>
              </ui:BindableSwitchElement>
            </ui:VisualElement>
            <ui:TemplateContainer template="layout-divider-solid-horizontal"/>
            <ui:VisualElement class="card-states-modal-states-chose-nations margin-bottom-16">
              <ui:SIScrollView>
                <ui:VisualElement class="frame-states-chose-nations-without-draft-restrictions">
                  <ui:SIRadioGroup class="segmented-button-group" selection-binding="continent"/>
                  <ui:SIVisible data-binding="config.ShowSlotsWarning">
                    <ui:TemplateContainer class="margin-top-8" template="SlotInfoWidget"/>
                  </ui:SIVisible>
                  <ui:SIVisible>
                    <ui:BindableSwitchElement class="margin-top-8 margin-bottom-8">
                      <ui:VisualElement class="row-direction-normal background-semi-transparent">
                        <ui:VisualElement class="ui-concepts-more-info_small margin-right-16"/>
                        <ui:SIText class="body-regular-14px-regular" text-binding="[VisualFunction]"/>
                      </ui:VisualElement>
                      <ui:VisualElement class="row-direction-normal background-red">
                        <ui:VisualElement class="ui-concepts-warning_small margin-right-16"/>
                        <ui:SIText class="body-regular-14px-regular" text-binding="[VisualFunction]"/>
                      </ui:VisualElement>
                    </ui:BindableSwitchElement>
                  </ui:SIVisible>
                  <ui:TemplateContainer template="ActiveNationContinent"/>
                </ui:VisualElement>
                <ui:VisualElement class="frame-states-chose-nations-without-draft-restrictions">
                  <ui:StreamedObjectList data-binding="SetupGame.ContinentalRuleGroups"/>
                </ui:VisualElement>
              </ui:SIScrollView>
            </ui:VisualElement>
            <ui:VisualElement class="row-direction-normal">
              <ui:SIImage class="tab-title--menu-true margin-right-global-gap-small"/>
              <ui:SIText class="global-text-focus-women-primary" text-binding="[VisualFunction]"/>
            </ui:VisualElement>
            <ui:TemplateContainer class="margin-top-12 margin-bottom-12 text-only" template="layout-divider-solid-horizontal"/>
            <ui:VisualElement>
              <ui:VisualElement class="row-direction-normal">
                <ui:VisualElement class="margin-right-global-gap-regular">
                  <ui:SIButton class="buttons-button-secondary-default">
                    <ui:SIText class="button-regular buttons-global-primary-text sitext-hover-cursor" text-binding="[VisualFunction]"/>
                  </ui:SIButton>
                </ui:VisualElement>
                <ui:VisualElement>
                  <ui:SIButton class="buttons-button-primary-default">
                    <ui:SIText class="button-regular buttons-global-primary-text sitext-hover-cursor" text-binding="[VisualFunction]"/>
                  </ui:SIButton>
                </ui:VisualElement>
              </ui:VisualElement>
            </ui:VisualElement>
          </ui:VisualElement>
        </ui:VisualElement>
      </ui:VisualElement>
    </ui:BindingVariables>
  </ui:BindingRoot>
</ui:UXML>