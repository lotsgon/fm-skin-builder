<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
  <ui:BindingRoot>
    <ui:VisualElement>
      <ui:SITile class="sicard">
        <ui:VisualElement/>
        <ui:VisualElement>
          <ui:VisualElement>
            <ui:VisualElement class="row-direction-normal margin-right-global-gap-large margin-left-global-gap-large">
              <ui:SIAspectRatioFitter>
                <ui:SIButton class="sibutton margin-right-global-gap-none margin-left-global-gap-none padding-right-global-padding-none padding-left-global-padding-none"/>
              </ui:SIAspectRatioFitter>
              <ui:SIText class="sitile__title white"/>
            </ui:VisualElement>
          </ui:VisualElement>
          <ui:BindingRemapper>
            <ui:TabGroup>
              <ui:ButtonGroup class="row-direction-normal">
                <ui:SIButton class="sipreferencestab"/>
                <ui:SIButton class="sipreferencestab"/>
                <ui:SIButton class="sipreferencestab"/>
              </ui:ButtonGroup>
              <ui:SwitchElement>
                <ui:DebugPropertiesElement>
                  <ui:FMActionButton/>
                </ui:DebugPropertiesElement>
                <ui:BindingVariables>
                  <ui:VisualElement class="row-direction-normal">
                    <ui:Label class="sitext"/>
                    <ui:SIDropdown/>
                  </ui:VisualElement>
                  <ui:VisualElement class="row-direction-normal">
                    <ui:VisualElement>
                      <ui:SIText class="sitext" text-binding="nationaltc.DebugSelectedSquadTitle"/>
                      <ui:StreamedTable data-binding="nationaltc.DebugSquadToBeSelected"/>
                    </ui:VisualElement>
                    <ui:VisualElement>
                      <ui:SIText class="sitext" text-binding="[VisualFunction]"/>
                      <ui:SIScrollView>
                        <ui:StreamedTable data-binding="nationaltc.DebugPlayersConsideredForSlot"/>
                      </ui:SIScrollView>
                      <ui:Label class="sitext margin-right-global-gap-none margin-left-global-gap-none"/>
                      <ui:SIText class="sitext" text-binding="nationaltc.DebugFormationsForSelectionString"/>
                    </ui:VisualElement>
                    <ui:VisualElement>
                      <ui:Label class="sitext margin-right-global-gap-none margin-left-global-gap-none"/>
                      <ui:SIScrollView>
                        <ui:SIText class="sitext" text-binding="nationaltc.DebugUnavailablePlayersString"/>
                      </ui:SIScrollView>
                    </ui:VisualElement>
                  </ui:VisualElement>
                </ui:BindingVariables>
                <ui:VisualElement>
                  <ui:StreamedTable data-binding="nationaltc.DebugTransferValue.DebugTransferValue"/>
                  <ui:SIText text-binding="nationaltc.debugtransfervalue.Nation"/>
                  <ui:SIText text-binding="nationaltc.debugtransfervalue.Continent"/>
                  <ui:SIText text-binding="nationaltc.debugtransfervalue.WorldContinent"/>
                </ui:VisualElement>
              </ui:SwitchElement>
            </ui:TabGroup>
          </ui:BindingRemapper>
        </ui:VisualElement>
      </ui:SITile>
    </ui:VisualElement>
  </ui:BindingRoot>
</ui:UXML>