<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
  <ui:BindingRoot>
    <ui:VisualElement class="sipopup-container">
      <ui:VisualElement class="card-notifications-modal-dialog-quit-without-saving">
        <ui:VisualElement>
          <ui:VisualElement class="notifications--notifications-modal-dialog-default-default">
            <ui:VisualElement>
              <ui:VisualElement>
                <ui:SIText class="heading-small-16px text-middle-center" text-binding="dialog.Title"/>
                <ui:SIText class="body-regular-14px-regular text-middle-center" text-binding="dialog.Description"/>
              </ui:VisualElement>
              <ui:SIVisible data-binding="dialog.ShowDynamicContent">
                <ui:VisualElement class="notifications--notifications-modal-dialog-dropdown-menu-default">
                  <ui:VisualElement class="row-direction-normal">
                    <ui:SIDropdown class="buttons-dropdown-icon-base-default">
                      <ui:VisualElement class="ui-concepts-tick_small"/>
                      <ui:SIText class="button-regular content-no-wrap content-ellipsis margin-right-global-gap-regular"/>
                      <ui:TemplateContainer class="controllers-keyboard-inputs margin-right-global-gap-regular" template="controllers-keyboard inputs"/>
                      <ui:VisualElement class="ui-concepts-add-to-list_small"/>
                      <ui:VisualElement class="ui-concepts-chevron-down_small"/>
                    </ui:SIDropdown>
                  </ui:VisualElement>
                </ui:VisualElement>
              </ui:SIVisible>
              <ui:SIVisible data-binding="dialog.ShowInputField">
                <ui:VisualElement class="notifications--notifications-modal-dialog-dropdown-menu-default base-template-grow">
                  <ui:BindableTextEditBox class="inputs-text-field-default-fill-available-width"/>
                </ui:VisualElement>
              </ui:SIVisible>
              <ui:VisualElement class="row-direction-normal">
                <ui:VisualElement>
                  <ui:VisualElement>
                    <ui:SIStyleSetter>
                      <ui:VisualElement class="notifications-modal-dialog-actions-default-type-confirm">
                        <ui:SIVisible data-binding="dialog.HasQuaternaryButton">
                          <ui:VisualElement class="margin-right-global-gap-regular">
                            <ui:SIButton class="buttons-button-secondary-default">
                              <ui:SIText class="button-regular content-ellipsis content-no-wrap content-ellipsis margin-right-global-gap-none" text-binding="dialog.QuaternaryButtonText"/>
                            </ui:SIButton>
                          </ui:VisualElement>
                        </ui:SIVisible>
                        <ui:SIVisible data-binding="dialog.HasTertiaryButton">
                          <ui:VisualElement class="margin-right-global-gap-regular">
                            <ui:SIButton class="buttons-button-secondary-default">
                              <ui:SIText class="button-regular content-ellipsis content-no-wrap content-ellipsis margin-right-global-gap-none" text-binding="dialog.TertiaryButtonText"/>
                            </ui:SIButton>
                          </ui:VisualElement>
                        </ui:SIVisible>
                        <ui:SIVisible data-binding="dialog.HasSecondaryButton">
                          <ui:TemplateContainer class="margin-right-global-gap-regular" template="buttons-button-secondary-default"/>
                        </ui:SIVisible>
                        <ui:SIVisible data-binding="dialog.HasPrimaryButton">
                          <ui:TemplateContainer class="margin-right-global-gap-regular" template="buttons-button-primary-default"/>
                        </ui:SIVisible>
                      </ui:VisualElement>
                    </ui:SIStyleSetter>
                  </ui:VisualElement>
                </ui:VisualElement>
              </ui:VisualElement>
            </ui:VisualElement>
            <ui:SIVisible data-binding="dialog.ShowCheckbox">
              <ui:VisualElement class="notifications--notifications-modal-dialog-checkbox-message-setting-default padding-right-global-padding-huge padding-left-global-padding-huge">
                <ui:VisualElement class="layout-layout-divider-solid-horizontal"/>
                <ui:VisualElement class="inputs-switch-checkbox-label padding-right-global-padding-huge padding-left-global-padding-huge">
                  <ui:VisualElement class="inputs-switch-checkbox-label">
                    <ui:SICheckBox class="inputs-switch-checkbox-default-switch-false--icon-none" selection-binding="doNotShowAgain"/>
                    <ui:SIText class="body-small-12px-regular global-text-secondary" text-binding="[VisualFunction]"/>
                  </ui:VisualElement>
                </ui:VisualElement>
              </ui:VisualElement>
            </ui:SIVisible>
          </ui:VisualElement>
        </ui:VisualElement>
      </ui:VisualElement>
    </ui:VisualElement>
  </ui:BindingRoot>
</ui:UXML>