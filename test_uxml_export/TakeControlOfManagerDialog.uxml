<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
  <ui:BindingRoot>
    <ui:BindingSetter>
      <ui:VisualElement>
        <ui:VisualElement class="notifications--notifications-modal-dialog-default-default">
          <ui:VisualElement>
            <ui:VisualElement>
              <ui:SIText class="heading-small-16px" text-binding="[VisualFunction]"/>
              <ui:BindableSwitchElement data-binding="haspassword">
                <ui:SIText class="body-regular-14px-regular margin-right-global-gap-none margin-left-global-gap-none" text-binding="[VisualFunction]"/>
                <ui:VisualElement>
                  <ui:SIText class="body-regular-14px-regular margin-right-global-gap-none margin-left-global-gap-none" text-binding="[VisualFunction]"/>
                  <ui:TemplateContainer template="inputs-text-field-default-default"/>
                </ui:VisualElement>
              </ui:BindableSwitchElement>
            </ui:VisualElement>
            <ui:TemplateContainer class="notifications-modal-dialog-actions-default-type-confirm row-direction-inverted" template="notifications-modal-dialog-actions-default"/>
          </ui:VisualElement>
        </ui:VisualElement>
      </ui:VisualElement>
    </ui:BindingSetter>
  </ui:BindingRoot>
</ui:UXML>