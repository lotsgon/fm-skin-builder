<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
  <ui:BindingRoot class="base-template-grow">
    <ui:BindingRemapper class="base-template-grow" binding-mappings="human=game.GameCurrentManager">
      <ui:BindingVariables class="base-template-grow">
        <ui:BindingSetter class="base-template-grow">
          <ui:BindableSwitchElement class="base-template-grow" data-binding="config.ShowSimplifiedTrainingUnits">
            <ui:TemplateContainer class="base-template-grow" template="FigmaBaseReportTabbedTemplate"/>
            <ui:BindableSwitchElement class="base-template-grow" data-binding="config.ShowPhonePanels">
              <ui:TemplateContainer class="base-template-grow" template="FigmaBaseReportTabbedTemplate"/>
              <ui:TemplateContainer class="base-template-grow" template="FigmaBaseReportTemplate"/>
            </ui:BindableSwitchElement>
          </ui:BindableSwitchElement>
        </ui:BindingSetter>
      </ui:BindingVariables>
    </ui:BindingRemapper>
  </ui:BindingRoot>
</ui:UXML>