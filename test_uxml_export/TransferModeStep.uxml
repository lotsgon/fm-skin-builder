<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
  <ui:BindingVariables>
    <ui:VisualElement>
      <ui:VisualElement>
        <ui:VisualElement class="align-items-start">
          <ui:VisualElement class="row-direction-normal align-items-start">
            <ui:VisualElement class="align-items-start">
              <ui:SIText class="heading-regular-18px heading-small-16px" text-binding="[VisualFunction]"/>
              <ui:SIText class="sitextmedium body-small-12px-regular global-text-secondary padding-left-global-padding-none" text-binding="[VisualFunction]"/>
            </ui:VisualElement>
          </ui:VisualElement>
          <ui:VisualElement class="align-items-end">
            <ui:VisualElement>
              <ui:SIRadioGroup class="vertical-radio-group continent-selection-radio-group" selection-binding="setupgame.futuretransfermode"/>
            </ui:VisualElement>
          </ui:VisualElement>
        </ui:VisualElement>
      </ui:VisualElement>
    </ui:VisualElement>
  </ui:BindingVariables>
</ui:UXML>