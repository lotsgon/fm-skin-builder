<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
  <ui:BindingRoot class="base-template-grow">
    <ui:BindableSwitchElement data-binding="config.ShowPhonePanels">
      <ui:VisualElement>
        <ui:SIVisible data-binding="const.IsFinal">
          <ui:BindingRemapper binding-mappings="competition=Club.Division">
            <ui:TemplateContainer template="QuickFlick"/>
          </ui:BindingRemapper>
        </ui:SIVisible>
        <ui:TemplateContainer class="base-template-grow" template="FigmaBaseCardTemplate"/>
      </ui:VisualElement>
      <ui:TemplateContainer class="base-template-grow" template="FigmaBaseCardTabbedTemplate"/>
    </ui:BindableSwitchElement>
  </ui:BindingRoot>
</ui:UXML>