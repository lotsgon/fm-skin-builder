<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
  <ui:BindingRoot>
    <ui:VisualElement class="modal-sheet-background">
      <ui:VisualElement class="panel-load-game-style-bar">
        <ui:VisualElement class="row-direction-normal margin-right-global-gap-none margin-left-global-gap-none">
          <ui:Rotator>
            <ui:SIImage class="special-icons-spinner-spinner"/>
          </ui:Rotator>
          <ui:SIText class="heading-small-16px padding-left-global-padding-extra-large" text-binding="[VisualFunction]"/>
        </ui:VisualElement>
      </ui:VisualElement>
    </ui:VisualElement>
  </ui:BindingRoot>
</ui:UXML>