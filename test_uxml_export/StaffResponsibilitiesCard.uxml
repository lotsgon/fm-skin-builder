    <ui:BindingRemapper class="base-template-grow" binding-mappings="teamsettings=teamcontainer.TeamSettingsEditSession;responsibilitylinkdata=fmconstants.ResponsibilitiesStaffLink">
      <ui:BindableSwitchElement class="flex-grow-class" data-binding="config.UseCutDownStaffResponsibilities">
        <ui:TemplateContainer class="flex-grow-class" template="FigmaBaseToolTabbedTemplate"/>
