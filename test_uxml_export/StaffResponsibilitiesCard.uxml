          <ui:TemplateContainer class="base-template-grow" template="FigmaBaseCardTemplate"/>
          <ui:TemplateContainer class="quick-action-buttons-template-layout" template="QuickActionMenuButtons"/>
        </ui:VisualElement>
      </ui:BindableSwitchElement>
    </ui:BindingRemapper>
  </ui:BindingRoot>
</ui:UXML>