<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
  <ui:BindingRoot>
    <ui:BindingRemapper binding-mappings="human=game.GameCurrentManager;club=game.GameCurrentManager.Club;team=game.GameCurrentManagerClubTeam">
      <ui:VisualElement>
        <ui:VisualElement class="notifications-modal-large-default-assign-a-scout">
          <ui:VisualElement>
            <ui:VisualElement>
              <ui:SIVisible data-binding="[VisualFunction]">
                <ui:SIText class="heading-small-16px" text-binding="[VisualFunction]"/>
              </ui:SIVisible>
              <ui:SIVisible data-binding="[VisualFunction]">
                <ui:SIText class="heading-small-16px" text-binding="[VisualFunction]"/>
              </ui:SIVisible>
              <ui:SIText class="body-regular-14px-regular"/>
            </ui:VisualElement>
            <ui:TemplateContainer class="layout-divider-solid-horizontal" template="layout-divider-solid-horizontal"/>
            <ui:VisualElement class="border-color-layout-modules-block-border-colour">
              <ui:SIVisible data-binding="[VisualFunction]">
                <ui:TemplateContainer class="row-direction-normal" template="inputs-switch-checkbox-label"/>
                <ui:SIEnabled data-binding="focusitem.AnyScout">
                  <ui:StreamedTable class="height-100-percent" data-binding="Team.Club.Scouts"/>
                </ui:SIEnabled>
              </ui:SIVisible>
              <ui:SIVisible data-binding="[VisualFunction]">
                <ui:TemplateContainer class="row-direction-normal" template="inputs-switch-checkbox-label"/>
                <ui:SIEnabled data-binding="focusitem.AnyAnalyst">
                  <ui:StreamedTable class="height-100-percent" data-binding="Team.Club.Analysts"/>
                </ui:SIEnabled>
              </ui:SIVisible>
            </ui:VisualElement>
            <ui:TemplateContainer class="layout-divider-solid-horizontal" template="layout-divider-solid-horizontal"/>
            <ui:VisualElement class="row-direction-normal">
              <ui:VisualElement class="row-direction-normal">
                <ui:VisualElement class="ui-concepts-tick_small margin-right-global-gap-regular"/>
                <ui:SIVisible data-binding="[VisualFunction]">
                  <ui:SIText class="body-regular-14px-bold" text-binding="[VisualFunction]"/>
                </ui:SIVisible>
                <ui:SIVisible data-binding="[VisualFunction]">
                  <ui:SIText class="body-regular-14px-bold" text-binding="[VisualFunction]"/>
                </ui:SIVisible>
              </ui:VisualElement>
              <ui:VisualElement>
                <ui:VisualElement class="notifications-modal-dialog-actions-default-type-confirm row-direction-inverted">
                  <ui:TemplateContainer class="margin-right-global-gap-regular" template="button-secondary-label"/>
                  <ui:TemplateContainer class="margin-right-global-gap-regular" template="button-primary-label"/>
                </ui:VisualElement>
              </ui:VisualElement>
            </ui:VisualElement>
          </ui:VisualElement>
        </ui:VisualElement>
      </ui:VisualElement>
    </ui:BindingRemapper>
  </ui:BindingRoot>
</ui:UXML>