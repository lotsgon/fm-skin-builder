<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
  <ui:BindingRoot>
    <ui:TemplateContainer template="BaseFloatingBlocksTemplate">
      <ui:SIScrollView>
        <ui:VisualElement class="notifications-modal-large-default-default">
          <ui:VisualElement>
            <ui:VisualElement>
              <ui:BindingRemapper binding-mappings="competition=Fixture.Competition">
                <ui:TemplateContainer class="calendar-fixture-card" template="FixtureBackground"/>
              </ui:BindingRemapper>
              <ui:SIGradient/>
            </ui:VisualElement>
            <ui:VisualElement class="padding-right-global-padding-large padding-left-global-padding-large">
              <ui:VisualElement class="row-direction-normal">
                <ui:SIText class="body-small-12px-regular global-text-primary" text-binding="[VisualFunction]"/>
                <ui:SIImage class="margin-right-global-gap-small margin-left-global-gap-regular" data-binding="[VisualFunction]"/>
                <ui:SIText class="body-small-12px-regular global-text-primary" text-binding="Fixture.Competition.Name"/>
              </ui:VisualElement>
              <ui:VisualElement class="row-direction-normal">
                <ui:SIImage class="margin-right-global-gap-small margin-left-global-gap-none" data-binding="[VisualFunction]"/>
                <ui:SIText class="body-small-12px-regular global-text-primary" text-binding="Fixture.Stadium.Name"/>
                <ui:SIVisible data-binding="Fixture.HasResult">
                  <ui:VisualElement class="row-direction-normal">
                    <ui:SIText class="body-small-12px-regular global-text-secondary padding-left-global-padding-small" text-binding="Fixture.Attendance"/>
                    <ui:SIText class="body-small-12px-regular global-text-secondary padding-left-global-padding-small" text-binding="[VisualFunction]"/>
                  </ui:VisualElement>
                </ui:SIVisible>
              </ui:VisualElement>
              <ui:VisualElement class="row-direction-normal">
                <ui:VisualElement>
                  <ui:SIImage class="row-direction-normal margin-right-global-gap-regular margin-left-global-gap-regular" data-binding="[VisualFunction]"/>
                  <ui:SIText class="global-text-primary heading-small-16px" text-binding="Fixture.HomeTeam.Name"/>
                </ui:VisualElement>
                <ui:SIVisible data-binding="Fixture.HasResult">
                  <ui:SIText class="global-text-primary heading-xlarge-30px margin-right-global-gap-small margin-left-global-gap-small" text-binding="Fixture.Result"/>
                </ui:SIVisible>
                <ui:SIVisible data-binding="Fixture.HasResult">
                  <ui:SIText class="global-text-primary heading-xlarge-30px margin-right-global-gap-small margin-left-global-gap-small" text-binding="Fixture.KickoffTime"/>
                </ui:SIVisible>
                <ui:VisualElement>
                  <ui:SIImage class="row-direction-normal margin-right-global-gap-regular margin-left-global-gap-regular" data-binding="[VisualFunction]"/>
                  <ui:SIText class="global-text-primary heading-small-16px" text-binding="Fixture.AwayTeam.Name"/>
                </ui:VisualElement>
              </ui:VisualElement>
            </ui:VisualElement>
          </ui:VisualElement>
          <ui:TemplateContainer template="layout-divider-solid-horizontal"/>
          <ui:SIVisible data-binding="Fixture.HasResult">
            <ui:BindingVariables>
              <ui:TabGroup class="margin-right-global-gap-none margin-left-global-gap-none padding-right-global-padding-none padding-left-global-padding-none">
                <ui:ButtonGroup class="row-direction-normal margin-right-global-gap-extra-large margin-left-global-gap-extra-large" current-selected-id-binding="selectedtabid" selection-binding="selectedtab">
                  <ui:VisualElement>
                    <ui:SIButton class="row-direction-normal padding-right-global-padding-none padding-left-global-padding-none">
                      <ui:SIText class="content-no-wrap content-ellipsis body-small-12px-regular margin-right-global-gap-regular margin-left-global-gap-regular" text-binding="[VisualFunction]"/>
                      <ui:TemplateContainer class="controllers-keyboard-inputs margin-right-global-gap-regular" template="controllers-keyboard inputs"/>
                    </ui:SIButton>
                  </ui:VisualElement>
                  <ui:VisualElement>
                    <ui:SIButton class="row-direction-normal padding-right-global-padding-none padding-left-global-padding-none">
                      <ui:SIText class="content-no-wrap content-ellipsis margin-right-global-gap-regular margin-left-global-gap-regular" text-binding="[VisualFunction]"/>
                      <ui:TemplateContainer class="controllers-keyboard-inputs margin-right-global-gap-regular" template="controllers-keyboard inputs"/>
                    </ui:SIButton>
                  </ui:VisualElement>
                </ui:ButtonGroup>
                <ui:SwitchElement>
                  <ui:VisualElement>
                    <ui:SIScrollView>
                      <ui:BindingRemapper class="padding-right-global-padding-large padding-left-global-padding-large" binding-mappings="team=Fixture.ContextTeam">
                        <ui:SIText class="body-regular-14px-bold global-text-secondary" text-binding="[VisualFunction]"/>
                        <ui:StreamedObjectList data-binding="Fixture.TeamNews"/>
                      </ui:BindingRemapper>
                      <ui:BindingRemapper class="padding-right-global-padding-large padding-left-global-padding-large" binding-mappings="team=Fixture.TeamOpponent">
                        <ui:SIText class="body-regular-14px-bold global-text-secondary" text-binding="[VisualFunction]"/>
                        <ui:StreamedObjectList data-binding="Fixture.TeamNews"/>
                      </ui:BindingRemapper>
                    </ui:SIScrollView>
                  </ui:VisualElement>
                  <ui:VisualElement>
                    <ui:StreamedObjectList class="padding-right-global-padding-large padding-left-global-padding-large" data-binding="Fixture.MatchRulesList"/>
                  </ui:VisualElement>
                </ui:SwitchElement>
              </ui:TabGroup>
            </ui:BindingVariables>
          </ui:SIVisible>
          <ui:SIVisible data-binding="Fixture.HasResult">
            <ui:BindingVariables>
              <ui:TabGroup class="margin-right-global-gap-none margin-left-global-gap-none padding-right-global-padding-none padding-left-global-padding-none">
                <ui:ButtonGroup class="row-direction-normal margin-right-global-gap-extra-large margin-left-global-gap-extra-large" current-selected-id-binding="selectedtabid" selection-binding="selectedtab">
                  <ui:VisualElement>
                    <ui:SIButton class="row-direction-normal padding-right-global-padding-none padding-left-global-padding-none">
                      <ui:SIText class="content-no-wrap content-ellipsis body-small-12px-regular margin-right-global-gap-regular margin-left-global-gap-regular" text-binding="[VisualFunction]"/>
                      <ui:TemplateContainer class="controllers-keyboard-inputs margin-right-global-gap-regular" template="controllers-keyboard inputs"/>
                    </ui:SIButton>
                  </ui:VisualElement>
                  <ui:VisualElement>
                    <ui:SIButton class="row-direction-normal padding-right-global-padding-none padding-left-global-padding-none">
                      <ui:SIText class="content-no-wrap content-ellipsis margin-right-global-gap-regular margin-left-global-gap-regular" text-binding="[VisualFunction]"/>
                      <ui:TemplateContainer class="controllers-keyboard-inputs margin-right-global-gap-regular" template="controllers-keyboard inputs"/>
                    </ui:SIButton>
                  </ui:VisualElement>
                </ui:ButtonGroup>
                <ui:SwitchElement>
                  <ui:VisualElement class="row-direction-normal">
                    <ui:VisualElement class="row-direction-normal">
                      <ui:BindingVariables>
                        <ui:StreamedObjectList class="fixture-home-team" data-binding="Fixture.HomeEventInfos"/>
                      </ui:BindingVariables>
                      <ui:TemplateContainer class="margin-right-global-gap-regular margin-left-global-gap-regular" template="layout-divider-solid-vertical"/>
                      <ui:BindingVariables>
                        <ui:StreamedObjectList class="fixture-away-team" data-binding="Fixture.AwayEventInfos"/>
                      </ui:BindingVariables>
                    </ui:VisualElement>
                  </ui:VisualElement>
                  <ui:VisualElement>
                    <ui:StreamedObjectList class="padding-right-global-padding-regular padding-left-global-padding-regular" data-binding="Fixture.ContextTeamLineup"/>
                  </ui:VisualElement>
                </ui:SwitchElement>
              </ui:TabGroup>
            </ui:BindingVariables>
          </ui:SIVisible>
        </ui:VisualElement>
      </ui:SIScrollView>
    </ui:TemplateContainer>
  </ui:BindingRoot>
</ui:UXML>