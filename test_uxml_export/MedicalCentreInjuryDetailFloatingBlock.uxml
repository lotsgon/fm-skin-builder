<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
  <ui:BindingRoot>
    <ui:TemplateContainer template="BaseFloatingBlocksTemplate">
      <ui:VisualElement class="notifications--notifications-modal-dialog-default-default">
        <ui:VisualElement class="row-direction-normal">
          <ui:VisualElement>
            <ui:SIStyleSetter>
              <ui:SIImage/>
            </ui:SIStyleSetter>
            <ui:SIStyleSetter>
              <ui:SIImage/>
            </ui:SIStyleSetter>
          </ui:VisualElement>
          <ui:VisualElement class="margin-left-global-gap-regular">
            <ui:VisualElement class="row-direction-normal">
              <ui:SIText class="heading-small-16px" text-binding="InjuryDescription"/>
              <ui:VisualElement class="row-direction-normal"/>
            </ui:VisualElement>
            <ui:SIText class="body-small-12px-regular global-text-secondary" text-binding="injurytimemissed"/>
          </ui:VisualElement>
        </ui:VisualElement>
        <ui:TemplateContainer template="layout-divider-solid-horizontal"/>
        <ui:VisualElement class="row-direction-normal">
          <ui:SIText class="body-small-12px-bold margin-right-global-gap-regular" text-binding="[VisualFunction]"/>
          <ui:TemplateContainer template="PlayerInjurySeverityCell"/>
        </ui:VisualElement>
      </ui:VisualElement>
    </ui:TemplateContainer>
  </ui:BindingRoot>
</ui:UXML>